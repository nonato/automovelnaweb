<h1>New vehicle_version</h1>

<% form_for(@vehicle_version) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :vehicle_id %><br />
    <%= f.collection_select :vehicle_id, Car.all, :id, :model %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', vehicle_versions_path %>