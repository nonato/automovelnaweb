<h1>Listing vehicle_advertisements</h1>

<table>
  <tr>
    <th>Customer</th>
    <th>Vehicle version</th>
    <th>Year</th>
    <th>Price</th>
    <th>Km</th>
  </tr>

<% @vehicle_advertisements.each do |vehicle_advertisement| %>
  <tr>
    <td><%=h vehicle_advertisement.customer.name %></td>
    <td><%=h vehicle_advertisement.vehicle_version.maker_model_version %></td>
    <td><%=h vehicle_advertisement.year %></td>
    <td><%=h vehicle_advertisement.price %></td>
    <td><%=h vehicle_advertisement.km %></td>
    <td><%= link_to 'Show', vehicle_advertisement %></td>
    <td><%= link_to 'Edit', edit_vehicle_advertisement_path(vehicle_advertisement) %></td>
    <td><%= link_to 'Destroy', vehicle_advertisement, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New vehicle_advertisement', new_vehicle_advertisement_path %>